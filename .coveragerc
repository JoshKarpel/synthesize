[run]

branch = True

source =
    src/
    tests/

[report]

skip_empty = True
show_missing = True
sort = -cover

exclude_lines =
    def __repr__
    if self\.debug

    case never:
    assert_never
    raise AssertionError
    raise NotImplementedError

    if 0:
    if False:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @overload
    assert False

    pragma: unreachable
    pragma: debugging
    pragma: never runs
